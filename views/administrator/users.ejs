                            </article>
                            <p id="form-users-email-helper" class="helper"></p>
                        </section>
                        <div class="form-group">
                            <label for="extension-telephone" class="form-label">Teléfono del país</label>
                            <div class="relative">
                                <select name="extension" class="extension-phone-number form-field" autocomplete="tel-country-code"></select>
                                <i class="fa-solid fa-globe icon-input"></i>
                            </div>
                            <p id="form-users-extension-helper" class="helper"></p>
                        </div>
                        <section class="form-group">
