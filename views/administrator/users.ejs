                            <label for="email" class="form-label">Correo Electronico</label>
                            <article class="relative">
                                <input type="email" name="email" class="form-field" required>
                                <i class="fa-regular fa-envelope icon-input"></i>
                            </article>
                            <p id="form-administrators-email-helper" class="helper"></p>
                        </section>
                        <section class="form-group">
                            <label for="telephone" class="form-label">Telefono</label>
                            <article class="relative">
                                <input type="tel" name="telephone" class="form-field" required>
                                <i class="fa-solid fa-phone icon-input"></i>
                            </article>
                            <p id="form-administrators-telephone-helper" class="helper"></p>
                        </section>
                        <section class="form-group">
                            <label for="birthdate" class="form-label">Area | Rol</label>
                            <article class="relative">
                                <input type="text" name="area" class="form-field" required>
                                <i class="fa-solid fa-layer-group icon-input"></i>
                            </article>
                            <p id="form-administrators-area-helper" class="helper"></p>
                        </section>
                        <section class="form-group">
                            <label for="pass" class="form-label">Contraseña</label>
                            <article class="relative">
                                <input type="password" name="pass" class="form-field" autocomplete="new-password" required>
                                <i class="fa-solid fa-lock icon-input"></i>
                            </article>
                            <p id="form-administrators-pass-helper" class="helper"></p>
                        </section>
                        <section class="form-group">
                            <label for="confirm-pass" class="form-label">Confirmación de Contraseña</label>
                            <article class="relative">
                                <input type="password" name="confirm-pass" class="form-field" required>
                                <i class="fa-solid fa-unlock icon-input"></i>
                            </article>
                            <p id="form-administrators-confirm-pass-helper" class="helper"></p>
                        </section>
                        <section class="form-buttons">
                            <button id="button-create-administrator" type="button" class="button-outline-gray text-blue m-0 w-100">Crear <i class="fa-solid fa-user-plus"></i></button>
                            <button id="button-update-administrator" type="button" class="button-outline-gray text-blue">Actualizar <i class="fa-solid fa-user-pen"></i></button>
                            <button id="button-delete-administrator" type="button" class="button-outline-gray text-red">Eliminar <i class="fa-solid fa-delete-left"></i></button>
                        </section>
                    </form>
                </article>
            </section>
        </section>
    </main>
